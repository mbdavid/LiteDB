			<KeyWords name = "GlobalConstructs" bold="false" italic="false" color="Blue">
				<Key word = "Namespace" />
				<Key word = "Class" />
				<Key word = "Imports" />
				<Key word = "Implements" />
				<Key word = "Inherits" />
