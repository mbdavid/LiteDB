				<Key word = "EndIf" />
				
				<Key word = "For" />
				<Key word = "To" />
				<Key word = "Step" />
				<Key word = "Each" />
				<Key word = "In" />
				<Key word = "Next" />
				
				<Key word = "Select" />
				<Key word = "Case" />
			</KeyWords>
			
			<KeyWords name = "ContextKeywords" bold="false" italic="false" color="Blue">
				<Key word = "Ansi" />
				<Key word = "Auto" />
				<Key word = "Unicode" />
				<Key word = "Preserve" />
				<Key word = "Until" />
			</KeyWords>
		</RuleSet>
		
		<RuleSet name = "PreprocessorSet" ignorecase = "true">
			<Delimiters>~!@%^*()-+=|\#/{}[]:;"'&lt;&gt;,.?</Delimiters>
			
			<KeyWords name = "PreProcessor" bold="true" italic="false" color="Maroon">
				<Key word = "Const" />
				<Key word = "If" />
				<Key word = "ElseIf" /> 
				<Key word = "Else" />
				<Key word = "End" />
				<Key word = "ExternalSource" />
				<Key word = "Region" />
				<Key word = "ExternalChecksum" />
			</KeyWords>
		</RuleSet>
	</RuleSets>
</SyntaxDefinition>
